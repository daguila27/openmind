<%- include layouts/admin_header.ejs %>
            <h1>Usuarios en el sistema</h1>
            <table class="table">
                <tr>
                    <th width="50px">No</th>
                    <th>Username</th>
                    <th>Password</th>
                    <th>Action</th>
                </tr>
                               
                <% if(data.length){ 
                                
                 for(var i = 0;i < data.length;i++) { %>
                 
                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].username%></td>
                    <td><%=data[i].password%></td>
                    <td>
                        <a class="btn btn-danger btn-xs a-inside delete" href="user/delete/<%=data[i].username%>">- Borrar</a>
                    </td>
                </tr>
            <% }
            
             }else{ %>
                 <tr>
                    <td colspan="3">No contacts to call</td>
                 </tr>
            <% } %>

